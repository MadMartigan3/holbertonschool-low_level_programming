#include <stdio.h>

/**
 * main - Entry point
 *
 * Description: 'use the command putchar for display the alphabet'
 *
 *@putchar
 *
 * Return: Always 0 (Success)
*/

int main(void)
{
	char alphabet;

	for (alphabet='A'; alphabet <= 'Z'; alphabet++)
	{
		putchar(alphabet);
	}
	for (alphabet='a'; alphabet <= 'z'; alphabet++)
	{
		putchar(alphabet);
	}
	putchar('\n');
	return (0);
}
